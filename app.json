{
  "expo": {
    "name": "back-log",
    "description": "A simple place to keep track of your video game library",
    "slug": "back-log",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.json",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pompouscatstudios.backlog"
    },
    "android": {
      "package": "com.backlog.backlog"
    }
  }
}
